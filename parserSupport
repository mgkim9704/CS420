from typing import List, Union, NamedTuple, Optional, NewType, Tuple
from enum import Enum

class Expr:
	pass

class directDeclarator(Expr):
	def __init__(self, type):
		self.type = type
		self.value = []
	def add(self, val):
		self.value.append(val)

class myList():
	def __init__(self):
		self.list = []
	def add(self, val):
		self.list.append(val)

